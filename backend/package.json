{
  "name": "ai-test-generator-backend",
  "version": "1.0.0",
  "description": "Backend API for AI-Enhanced Test Case Generator",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "babel src -d dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:status": "node src/utils/db-cli.js status",
    "db:seed": "node src/utils/db-cli.js seed",
    "db:clear": "node src/utils/db-cli.js clear",
    "db:backup": "node src/utils/db-cli.js backup",
    "db:restore": "node src/utils/db-cli.js restore",
    "migrate": "node src/utils/db-cli.js migrate",
    "test:db": "node src/utils/test-db.js",
    "migrate": "node src/utils/migrate.js",
    "seed": "node src/utils/seed.js"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "jsonwebtoken": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "mongoose": "^7.4.1",
    "pg": "^8.11.1",
    "sequelize": "^6.32.1",
    "@babel/parser": "^7.22.7",
    "@babel/traverse": "^7.22.8",
    "@babel/types": "^7.22.5",
    "esprima": "^4.0.1",
    "escodegen": "^2.1.0",
    "acorn": "^8.10.0",
    "acorn-walk": "^8.2.0",
    "axios": "^1.4.0",
    "huggingface": "^0.1.5",
    "@huggingface/inference": "^2.6.1",
    "winston": "^3.10.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-async-errors": "^3.1.1",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "fs-extra": "^11.1.1",
    "mime-types": "^2.1.35",
    "node-cache": "^5.1.2",
    "redis": "^4.6.7",
    "bull": "^4.11.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "babel-jest": "^29.6.1",
    "mongodb-memory-server": "^8.13.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js",
      "!src/config/*.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "18"
          }
        }
      ]
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "ai",
    "test-generation",
    "api",
    "express",
    "hugging-face"
  ],
  "author": "AI Test Generator Team",
  "license": "MIT"
}